{
  "name": "angular-migration-phase-1",
  "version": "1.0.0",
  "description": "Migration Angular 5 vers Angular 8 - Phase Critique (RxJS, Build System, TypeScript)",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "migrate": "npm run build && node dist/index.js",
    "validate": "npm run test && npm run build && npm run lint",
    "rollback": "node dist/rollback.js",
    "demo": "node dist/demo.js"
  },
  "keywords": [
    "angular",
    "migration",
    "phase1",
    "angular5",
