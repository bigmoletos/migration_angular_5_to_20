  "license": "MIT",
  "dependencies": {
    "@angular/cli": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "rxjs": "^6.6.0",
    "typescript": "^3.4.5",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/jest": "^24.0.0",
    "typescript": "^3.4.5",
    "ts-node": "^8.0.0",
    "jest": "^24.0.0",
    "eslint": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0"
