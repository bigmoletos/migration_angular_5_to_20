{
  "name": "angular-migration-5-to-20",
  "version": "1.0.0",
  "description": "Migration Angular 5 vers Angular 20 - Monorepo avec 4 phases",
  "private": true,
  "workspaces": [
    "phase-1-angular-5-to-8",
    "phase-2-angular-8-to-12",
    "phase-3-angular-12-to-16",
    "phase-4-angular-16-to-20",
    "shared",
    "tools"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:phase1": "npm run build --workspace=phase-1-angular-5-to-8",
    "build:phase2": "npm run build --workspace=phase-2-angular-8-to-12",
    "build:phase3": "npm run build --workspace=phase-3-angular-12-to-16",
    "build:phase4": "npm run build --workspace=phase-4-angular-16-to-20",
    "test": "npm run test --workspaces",
    "test:phase1": "npm run test --workspace=phase-1-angular-5-to-8",
    "test:phase2": "npm run test --workspace=phase-2-angular-8-to-12",
    "test:phase3": "npm run test --workspace=phase-3-angular-12-to-16",
    "test:phase4": "npm run test --workspace=phase-4-angular-16-to-20",
    "lint": "npm run lint --workspaces",
    "migrate:phase1": "npm run migrate --workspace=phase-1-angular-5-to-8",
    "migrate:phase2": "npm run migrate --workspace=phase-2-angular-8-to-12",
    "migrate:phase3": "npm run migrate --workspace=phase-3-angular-12-to-16",
    "migrate:phase4": "npm run migrate --workspace=phase-4-angular-16-to-20",
    "migrate:full": "npm run migrate:phase1 && npm run migrate:phase2 && npm run migrate:phase3 && npm run migrate:phase4",
    "validate:phase1": "npm run validate --workspace=phase-1-angular-5-to-8",
    "validate:phase2": "npm run validate --workspace=phase-2-angular-8-to-12",
    "validate:phase3": "npm run validate --workspace=phase-3-angular-12-to-16",
    "validate:phase4": "npm run validate --workspace=phase-4-angular-16-to-20",
    "validate:all": "npm run validate:phase1 && npm run validate:phase2 && npm run validate:phase3 && npm run validate:phase4",
    "rollback:phase1": "npm run rollback --workspace=phase-1-angular-5-to-8",
    "rollback:phase2": "npm run rollback --workspace=phase-2-angular-8-to-12",
    "rollback:phase3": "npm run rollback --workspace=phase-3-angular-12-to-16",
    "rollback:phase4": "npm run rollback --workspace=phase-4-angular-16-to-20",
    "orchestrate": "node tools/orchestrator/index.js",
    "demo": "node demo.js"
  },
  "keywords": [
    "angular",
    "migration",
    "automation",
    "typescript",
    "monorepo",
    "phases",
    "java",
    "spring-boot"
  ],
  "author": "Votre nom",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}