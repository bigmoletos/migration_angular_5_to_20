{
  "name": "angular-migration-phase-3",
  "version": "1.0.0",
  "description": "Migration Angular 12 vers Angular 16 - Modernisation (Standalone, inject(), Signals, Typed Forms)",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "migrate": "npm run build && node dist/index.js",
    "validate": "npm run test && npm run build && npm run lint",
    "rollback": "node dist/rollback.js",
    "demo": "node dist/demo.js"
  },
  "keywords": [
    "angular",
    "migration",
    "phase3",
    "angular12",
    "angular16",
    "standalone",
    "inject",
    "signals",
    "typed-forms",
    "java",
    "spring-boot"
  ],
  "author": "Votre nom",
  "license": "MIT",
  "dependencies": {
    "@angular/cli": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "rxjs": "^7.8.0",
    "typescript": "^5.0.0",
    "tslib": "^2.6.0",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/jest": "^29.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "peerDependencies": {
    "rxjs": "^7.8.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
