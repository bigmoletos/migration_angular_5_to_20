  "description": "Migration Angular 8 vers Angular 12 - Stabilisation (Ivy, Webpack 5, Optimisations)",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
