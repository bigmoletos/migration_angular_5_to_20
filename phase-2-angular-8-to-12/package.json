    "@angular/cli": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "rxjs": "^6.6.0",
    "typescript": "^4.2.4",
    "tslib": "^2.3.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/jest": "^26.0.0",
    "typescript": "^4.2.4",
    "ts-node": "^10.0.0",
    "jest": "^27.0.0",
    "eslint": "^7.0.0",
